{
    "build": {
        "dockerfile": "./Dockerfile",
        "context": "."
    },
    "features": {
        "devcontainers/features/common@v0.0.2": {
            "username": "codespace",
            "user_uid": "1000",
            "user_gid": "1000"
        },
        "devcontainers/features/dotnet@v0.0.2": {
            "version": "3",
            "install_using_apt": "false",
            "override_default_version": "false"
        },
        "devcontainers/features/dotnet@v0.0.2": {
            "version": "6",
            "install_using_apt": "false",
            "override_default_version": "true"
        },
        "devcontainers/features/hugo@v0.0.2": {
            "version": "latest"
        },
        "devcontainers/features/node@v0.0.2": {
            "version": "16"
        },
        "devcontainers/features/node@v0.0.2": {
            "version": "14"
        },
        "./local-features/nvs": "latest",
        "devcontainers/features/python@v0.0.2": {
            "version": "3.9",
            "override_default_version": "false",
            "install_jupyterlab": "true"
        },
        "devcontainers/features/python@v0.0.2": {
            "version": "3.10",
            "override_default_version": "true",
            "install_jupyterlab": "true"
        },
        "./local-features/machine-learning-packages": "latest",
        "devcontainers/features/php@v0.0.2": {
            "version": "8.0.16",
            "install_composer": "true",
            "override_default_version": "false"
        },
        "devcontainers/features/php@v0.0.2": {
            "version": "8.1.4",
            "install_composer": "true",
            "override_default_version": "true"
        },
        "devcontainers/features/anaconda@v0.0.2": {
            "version": "latest"
        },
        "devcontainers/features/ruby@v0.0.2": {
            "version": "3.0"
        },
        "devcontainers/features/ruby@v0.0.2": {
            "version": "3.1"
        },
        "devcontainers/features/java@v0.0.2": {
            "version": "17"
        },
        "devcontainers/features/java@v0.0.2": {
            "version": "11"
        },
        "./local-features/maven": {
            "version": "3"
        },
        "devcontainers/features/sshd@v0.0.2": {
            "version": "latest"
        },
        "devcontainers/features/git-lfs@v0.0.2": {
            "version": "latest"
        },
        "devcontainers/features/github-cli@v0.0.2": {
            "version": "latest"
        },
        "devcontainers/features/docker-in-docker@v0.0.2": {
            "version": "latest"
        },
        "devcontainers/features/kubectl-helm-minikube@v0.0.2": {
            "version": "latest"
        },
        "devcontainers/features/git@v0.0.2": {
            "version": "latest"
        },
        "./local-features/gradle": "latest",
        "devcontainers/features/go@v0.0.2": {
            "version": "latest"
        }
        "./local-features/jekyll": "latest",
        "devcontainers/features/oryx@v0.0.2": "latest",
        "./local-features/setup-user": "latest"
    },
    "overrideFeatureInstallOrder": [
        "devcontainers/features/common@v0.0.2",

    ],
    "remoteUser": "codespace",
    "containerUser": "codespace",
    "overrideCommand": false,
    "mounts": [
        "source=codespaces-linux-var-lib-docker,target=/var/lib/docker,type=volume"
    ],
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined",
        "--privileged",
        "--init"
    ]
}
{
    "id": "maven",
    "name": "Maven (via SDKMAN!)",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "none",
                "3.8",
                "3.6",
                "3.5"
            ],
            "default": "latest",
            "description": "Select or enter a Maven version to install"
        }
    },
    "containerEnv": {
        "SDKMAN_DIR": "${SDKMAN_DIR:-\"/usr/local/sdkman\"}",
        "PATH": "${SDKMAN_DIR}/bin:${SDKMAN_DIR}/candidates/maven/current/bin:${PATH}"
    },
    "install": {
        "app": "",
        "file": "install.sh"
    }
}